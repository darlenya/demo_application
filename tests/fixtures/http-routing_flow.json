{
	"http-routing-flow": {
		"description": "Untars the TAR-Ball containing all the application files",

		"steps": {

			"adapterInboundHttp": {
				"type": "kronos-http-routing",
				"listener": "my_koa_service",
				"endpoints": {
					"fileReaderTar": {
						"method": "post",
						"path": "/file/tar",
						"target": "debugStep/in",
						"out": true
					}
				}
			},

			"debugStep": {
				"type": "debug-step"
			}


		}

	}
}
